{
  "name": "fill-the-paragraph",
  "displayName": "Fill the Paragraph",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fill-the-paragraph.fill"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "fill-the-paragraph.fill",
      "title": "Fill the Paragraph"
    }],
    "configuration": {
      "title": "Fill the Paragraph",
      "properties": {
        "fill-the-paragraph.maxLineLength": {
          "type": "number",
          "default": 120,
          "description": "Specifies the max length of characters per line to be split."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.2",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
